#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00DC39A0 .scope module, "FourBitUpCounter_tb" "FourBitUpCounter_tb" 2 1;
 .timescale 0 0;
v00DCEC78_0 .var "Clear", 0 0;
v00DCECD0_0 .var "Clk", 0 0;
v00DCED28_0 .net "Q", 3 0, v00DCEC20_0; 1 drivers
S_00DC2CA0 .scope module, "UUT" "FourBitUpCounter" 2 5, 3 1, S_00DC39A0;
 .timescale 0 0;
v00DC2D28_0 .net "Clear", 0 0, v00DCEC78_0; 1 drivers
v00DC2D80_0 .net "Clk", 0 0, v00DCECD0_0; 1 drivers
v00DCEC20_0 .var "Q", 3 0;
E_00DC7850 .event posedge, v00DC2D28_0, v00DC2D80_0;
    .scope S_00DC2CA0;
T_0 ;
    %wait E_00DC7850;
    %load/v 8, v00DC2D28_0, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v00DCEC20_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 0, 1, 0;
    %load/vp0 8, v00DCEC20_0, 4;
    %set/v v00DCEC20_0, 8, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00DC39A0;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "4bitUpCounter.vpd";
    %vpi_call 2 9 "$dumpvars";
    %end;
    .thread T_1;
    .scope S_00DC39A0;
T_2 ;
    %set/v v00DCECD0_0, 1, 1;
T_2.0 ;
    %delay 5, 0;
    %load/v 8, v00DCECD0_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00DCECD0_0, 0, 8;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_00DC39A0;
T_3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00DCEC78_0, 0, 1;
    %delay 15, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00DCEC78_0, 0, 0;
    %delay 40, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00DCEC78_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_00DC39A0;
T_4 ;
    %delay 60, 0;
    %vpi_call 2 31 "$finish";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "4bitUpCounter_tb.v";
    "4bitUpCounter.v";
